        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.514" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="297dp"
        android:layout_height="115dp"
        android:layout_marginEnd="56dp"
